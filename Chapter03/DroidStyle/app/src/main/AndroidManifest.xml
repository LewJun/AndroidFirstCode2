<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.droidstyle">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ShapeActivity"
            android:label="Shape" />
        <activity
            android:name=".SelectorActivity"
            android:label="Selector" />
        <activity
            android:name=".LayerListActivity"
            android:label="LayerList" />
        <activity
            android:name=".AnimationActivity"
            android:label="Animation" />
        <activity
            android:name=".RectangleActivity"
            android:label="Rectangle" />
        <activity android:name=".OvalActivity" />
        <activity android:name=".LineActivity" />
        <activity android:name=".RingActivity"></activity>
    </application>

</manifest>